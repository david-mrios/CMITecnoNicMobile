<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".editProfile">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="409dp"
        android:layout_height="729dp"
        android:layout_marginStart="1dp"
        android:layout_marginTop="1dp"
        android:layout_marginEnd="1dp"
        android:layout_marginBottom="1dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">


        <ImageButton
            android:id="@+id/imageButton"
            android:layout_width="42dp"
            android:layout_height="39dp"
            android:background="@drawable/circle_button"
            android:padding="8dp"
            android:scaleType="centerInside"
            app:srcCompat="@drawable/ic_back"
            tools:layout_editor_absoluteX="16dp"
            tools:layout_editor_absoluteY="44dp" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:text="Editar Perfil"
            android:textColor="@color/black"
            android:textSize="25sp"
            tools:layout_editor_absoluteX="131dp"
            tools:layout_editor_absoluteY="49dp" />

        <ImageButton
            android:id="@+id/imageButton2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/circle_button"
            app:srcCompat="@drawable/ic_profile"
            tools:layout_editor_absoluteX="159dp"
            tools:layout_editor_absoluteY="113dp" />

        <TextView
            android:id="@+id/textView9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Testing User"
            android:textColor="@color/black"
            android:textSize="20dp"
            tools:layout_editor_absoluteX="148dp"
            tools:layout_editor_absoluteY="211dp" />

        <TextView
            android:id="@+id/txtEmail"
            android:layout_width="139dp"
            android:layout_height="16dp"
            android:text="Nombre Completo"
            android:textColor="@color/gray_500"
            android:textSize="14sp"
            tools:layout_editor_absoluteX="1dp"
            tools:layout_editor_absoluteY="269dp" />

        <EditText
            android:id="@+id/editTextText"
            android:layout_width="404dp"
            android:layout_height="58dp"
            android:background="@android:color/transparent"
            android:drawableLeft="@drawable/ic_testing_user"
            android:drawablePadding="20dp"
            android:ems="10"
            android:padding="12dp"
            android:text="Testing User"
            android:textColor="@android:color/black"
            android:textSize="16sp"
            tools:layout_editor_absoluteX="2dp"
            tools:layout_editor_absoluteY="292dp" />

        <TextView
            android:id="@+id/textView10"
            android:layout_width="139dp"
            android:layout_height="16dp"
            android:text="Correo Electrónico"
            android:textColor="@color/gray_500"
            android:textSize="14sp"
            tools:layout_editor_absoluteX="1dp"
            tools:layout_editor_absoluteY="356dp" />

        <EditText
            android:id="@+id/editTextText2"
            android:layout_width="404dp"
            android:layout_height="58dp"
            android:background="@android:color/transparent"
            android:drawableLeft="@drawable/ic_email_mail_letter"
            android:drawablePadding="20dp"
            android:ems="10"
            android:padding="12dp"
            android:inputType="text"
            android:text="testing@gmail.com"
            android:textColor="@android:color/black"
            android:textSize="16sp"
            tools:layout_editor_absoluteX="2dp"
            tools:layout_editor_absoluteY="389dp" />

        <TextView
            android:id="@+id/textView11"
            android:layout_width="139dp"
            android:layout_height="16dp"
            android:text="Número de Teléfono"
            android:textColor="@color/gray_500"
            android:textSize="14sp"
            tools:layout_editor_absoluteX="1dp"
            tools:layout_editor_absoluteY="454dp" />

        <EditText
            android:id="@+id/editTextText3"
            android:layout_width="404dp"
            android:layout_height="58dp"
            android:background="@android:color/transparent"
            android:drawableLeft="@drawable/ic_phone"
            android:drawablePadding="20dp"
            android:ems="10"
            android:padding="12dp"
            android:inputType="text"
            android:text="+8801712663389"
            android:textColor="@android:color/black"
            android:textSize="16sp"
            tools:layout_editor_absoluteX="1dp"
            tools:layout_editor_absoluteY="484dp" />

        <TextView
            android:id="@+id/textView12"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:text="Se unió 21 Enero 2024"
            tools:layout_editor_absoluteX="134dp"
            tools:layout_editor_absoluteY="670dp" />


    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>